{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-shadow" : 0,
        "no-underscore-dangle" : 0,
        "no-tabs": 0,
        "max-len" : 0,
        "prefer-promise-reject-errors" : 0,
        "no-unused-vars": 0,
        "no-use-before-define" : 0,
        "no-restricted-globals": 0,
        "no-console": 0,
        "no-unused-expressions": 0,
        "array-callback-return": 0,
        "no-undef" : 0,
        "linebreak-style" : 0
    }
}
